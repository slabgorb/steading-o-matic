extends layout

block content
  .container
    .jumbotron
      h1 Register
    form(action="/local-reg", method="POST")
      .form-group
        label(for:'email') Email
        input.form-control(type='text', name='email')
      .form-group
        label(for:'password') Password
        input.form-control(type='password', name='password')
      
      button(type:'submit') Submit
